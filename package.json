{
  "name": "business-day-calendar",
  "version": "0.1.0",
  "description": "luxon wrapper for business days functionality",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run build:rolldown && npm run build:types",
    "build:rolldown": "rolldown -c",
    "build:types": "tsc --emitDeclarationOnly --allowJs --declaration --outDir dist --project tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brandnewteam/business-day-calendar.git"
  },
  "author": "3D Print <automation@3dprint.sm>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandnewteam/business-day-calendar/issues"
  },
  "homepage": "https://github.com/brandnewteam/business-day-calendar#readme",
  "peerDependencies": {
    "luxon": "3.x.x"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/luxon": "^3.7.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.32.0",
    "globals": "^16.3.0",
    "luxon": "3.x.x",
    "rolldown": "^1.0.0-beta.33",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
